{
  "name": "@shohojdhara/atomix",
  "version": "0.1.5",
  "description": "Atomix Design System - A modern component library for web applications",
  "main": "dist/js/atomix-${version}.react.cjs.js",
  "module": "dist/js/atomix-${version}.react.esm.js",
  "unpkg": "dist/js/atomix-${version}.react.js",
  "jsdelivr": "dist/js/atomix-${version}.react.js",
  "browser": "dist/js/atomix-${version}.react.js",
  "typings": "dist/types/react.d.ts",
  "sideEffects": [
    "*.css",
    "*.scss",
    "./dist/css/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/js/atomix-${version}.react.esm.js",
      "require": "./dist/js/atomix-${version}.react.cjs.js",
      "default": "./dist/js/atomix-${version}.react.esm.js"
    },
    "./package.json": "./package.json",
    "./css": {
      "types": "./dist/types/css.d.ts",
      "default": "./dist/css/atomix-${version}.css"
    },
    "./css/min": {
      "types": "./dist/types/css.d.ts",
      "default": "./dist/css/atomix-${version}.min.css"
    },
    "./scss": "./src/styles/index.scss"
  },
  "dependencies": {
    "@types/classnames": "^2.3.0",
    "@types/node": "^20.0.0",
    "@types/react-router-dom": "^5.3.3",
    "classnames": "^2.5.1",
    "phosphor-react": "^1.4.1",
    "prism-react-renderer": "^2.4.1",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/cli": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-webpack5": "^8.6.12",
    "@storybook/test": "^8.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "caniuse-lite": "^1.0.30001721",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.1.2",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "polished": "^4.2.2",
    "postcss": "^8.4.24",
    "postcss-loader": "^8.1.1",
    "sass-embedded": "^1.87.0",
    "sass-loader": "^16.0.5",
    "storybook": "^8.0.0",
    "style-loader": "^4.0.0",
    "terser": "^5.24.0",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^5.0.4",
    "webpack": "^5.99.7",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-node-externals": "^3.0.0",
    "@size-limit/preset-app": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "size-limit": "^11.0.0"
  },
  "scripts": {
    "analyze": "npx webpack --mode production --env target=components --env format=all --env analyze=true",
    "dev": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "predeploy": "npm run build-storybook && node scripts/prepare-deploy.js",
    "deploy": "gh-pages -d deploy",
    "prepublishOnly": "npm run build:components",
    "test:publish": "npm pack --dry-run",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "analyze:size": "npx size-limit",
    "analyze:bundle": "npm run analyze",
    "build:parallel": "npm-run-all --parallel build:cjs build:esm build:umd",
    "clean:dist": "rm -rf dist",
    "build:components": "npm-run-all clean:dist build:types --parallel build:react build:styles build:types",
    "build:react": "npx webpack --mode production --env target=components --env format=all",
    "build:styles": "npm run clean:dist && npx webpack --mode production --env target=styles",
    "build:cjs": "npx webpack --mode production --env target=components --env format=cjs",
    "build:esm": "npx webpack --mode production --env target=components --env format=esm",
    "build:umd": "npx webpack --mode production --env target=components --env format=umd",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "link:local": "npm link && echo 'Package linked locally. Use npm link @shohojdhara/atomix in your project to test it.'",
    "unlink:local": "npm unlink",
    "publish:rc": "npm publish --tag next",
    "publish:latest": "npm publish"
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "NPM_PUBLISHING.md"
  ],
  "keywords": [
    "ui",
    "components",
    "design-system",
    "react",
    "javascript",
    "typescript",
    "css",
    "scss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/liimonx/atomix.git"
  },
  "author": "liimonx <liimonx@shohojdhara.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liimonx/atomix/issues"
  },
  "homepage": "https://github.com/liimonx/atomix#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "scope": "@shohojdhara"
  }
}
